        "Vh = float(input(\"Insira o valor da hora trabalhada: \")) #Valor hora\n",
        "Th = float(input(\"Insira o total de horas trabalhadas no mês: \")) #Total horas\n",
        "\n",
        "Sb = Vh*Th #Salário bruto\n",
        "Di = (7.5/100)*Sb #Desconto INSS\n",
        "Sl = Sb-Di #Salário liquido\n",
        "\n",
        "print(\"Seu salário bruto é\",Sb)\n",
        "print(\"O desconto do INSS é de\",Di)\n",
        "print(\"Seu salário liquido é\",Sl)"
